{
  "containerDefinitions": [
    {
      "name": "task-app-container-nginx-backend",
      "image": "225176608006.dkr.ecr.ap-northeast-1.amazonaws.com/task-app-nginx:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [
        {
          "sourceContainer": "task-app-container-nest-backend"
        }
      ],
      "dependsOn": [
        {
          "containerName": "task-app-container-nest-backend",
          "condition": "START"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/task/backend/middleware",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "name": "task-app-container-nest-backend",
      "image": "225176608006.dkr.ecr.ap-northeast-1.amazonaws.com/task-app-backend:m",
      "cpu": 0,
      "portMappings": [],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/task/backend/app",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "task-app-task-backend",
  "taskRoleArn": "arn:aws:iam::225176608006:role/ecs_task_execution_role",
  "executionRoleArn": "arn:aws:iam::225176608006:role/ecs_task_execution_role",
  "networkMode": "awsvpc",

  "volumes": [],

  "placementConstraints": [],

  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "tags": []
}
